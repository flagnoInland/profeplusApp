<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.teacher_job.ResultTFStateOneFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/botones"
            style="@style/results_button_boxx">

            <Button
                android:id="@+id/btn_refresh_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/text_update_answer"
                style="@style/refresh_chart_button"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Button
                    android:layout_alignParentBottom="true"
                    android:id="@+id/btn_get_report"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    style="@style/results_app_button"
                    android:text="@string/text_get_report"/>


                <Button
                    android:layout_alignParentBottom="true"
                    android:id="@+id/btn_finish_one"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    style="@style/results_app_button"
                    android:text="@string/text_finish_one"/>


            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_above="@+id/botones"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <include layout="@layout/header_template4"></include>


            <include layout="@layout/subfragment_results_tf"/>

        </LinearLayout>







    </RelativeLayout>

</FrameLayout>
