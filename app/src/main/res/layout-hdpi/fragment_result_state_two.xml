<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/app_fondo"
    tools:context=".fragments.teacher_job.ResultStateTwoFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/botones"
            style="@style/results_button_box">

            <LinearLayout
                android:id="@+id/lay_normal"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Button
                    android:layout_alignParentBottom="true"
                    android:id="@+id/btn_finish_exit"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    style="@style/results_app_button"
                    android:text="@string/text_finish_exit"/>

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/twhite1"/>

                <Button
                    android:layout_alignParentBottom="true"
                    android:id="@+id/btn_start_two"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    style="@style/results_app_button"
                    android:text="@string/text_start_two"/>

                <Button
                    android:visibility="gone"
                    android:id="@+id/btn_refresh_result"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/text_update_answer"
                    style="@style/refresh_chart_button"/>

            </LinearLayout>

            <Button
                android:visibility="visible"
                android:layout_alignParentBottom="true"
                android:id="@+id/btn_finish_survey"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                style="@style/results_app_button"
                android:text="@string/text_finish_exit"/>





        </LinearLayout>


        <LinearLayout
            android:layout_above="@+id/botones"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <include layout="@layout/header_template4"></include>

            <include layout="@layout/subfragment_results"/>


        </LinearLayout>


    </RelativeLayout>

</FrameLayout>
